# Nusantara — Indonesia region data for Laravel
# Copy to your app's .env or set these in your environment.

# Storage: "file" (no DB) or "database"
NUSANTARA_DRIVER=file

# Cache TTL in seconds (0 = no cache). Recommended: 86400 for file, 3600 for database.
NUSANTARA_CACHE_TTL=86400

# Cache store name (leave empty for default)
NUSANTARA_CACHE_STORE=

# Custom data path for file driver (leave empty for package default)
NUSANTARA_DATA_PATH=

# Fuzzy search: min similarity 0–100
NUSANTARA_FUZZY_THRESHOLD=70

# Max search results
NUSANTARA_SEARCH_MAX_RESULTS=20
