{
    "name": "dimasdev/nusantara",
    "description": "Indonesia region data (Kepmendagri 2025) for Laravel â€” shipping address formatting, postal code lookup, fuzzy search, dual-mode storage (file/database). Usable worldwide in Laravel apps.",
    "keywords": [
        "laravel",
        "indonesia",
        "nusantara",
        "region",
        "wilayah",
        "provinsi",
        "kabupaten",
        "kecamatan",
        "kelurahan",
        "shipping",
        "address",
        "postal-code",
        "kode-pos",
        "address-formatter",
        "fuzzy-search"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dimas",
            "email": "your@email.com"
        }
    ],
    "require": {
